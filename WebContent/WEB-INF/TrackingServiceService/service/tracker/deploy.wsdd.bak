<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TrackingServiceService WSDL service -->

  <service name="TrackingService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://tracker.service"/>
      <parameter name="wsdlServiceElement" value="TrackingServiceService"/>
      <parameter name="schemaQualified" value="http://tracker.service"/>
      <parameter name="wsdlServicePort" value="TrackingService"/>
      <parameter name="className" value="service.tracker.TrackingServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TrackingService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="trackShipment" qname="operNS:trackShipment" xmlns:operNS="http://tracker.service" returnQName="retNS:trackShipmentReturn" xmlns:retNS="http://tracker.service" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:shipmentId" xmlns:pns="http://tracker.service" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="trackCar" qname="operNS:trackCar" xmlns:operNS="http://tracker.service" returnQName="retNS:trackCarReturn" xmlns:retNS="http://tracker.service" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:carChassisNumber" xmlns:pns="http://tracker.service" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="trackCar trackShipment"/>

  </service>
</deployment>
