<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ShipmentStationServiceService WSDL service -->

  <service name="ShipmentStationService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://shipment_station.service"/>
      <parameter name="wsdlServiceElement" value="ShipmentStationServiceService"/>
      <parameter name="schemaQualified" value="http://shipment_station.service,http://shipment.dto,http://part.dto"/>
      <parameter name="wsdlServicePort" value="ShipmentStationService"/>
      <parameter name="className" value="service.shipment_station.ShipmentStationServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ShipmentStationService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAllShipments" qname="operNS:getAllShipments" xmlns:operNS="http://shipment_station.service" returnQName="retNS:getAllShipmentsReturn" xmlns:retNS="http://shipment_station.service" returnType="rtns:ShipmentDTO" xmlns:rtns="http://shipment.dto" soapAction="" >
      </operation>
      <operation name="getShipment" qname="operNS:getShipment" xmlns:operNS="http://shipment_station.service" returnQName="retNS:getShipmentReturn" xmlns:retNS="http://shipment_station.service" returnType="rtns:ShipmentDTO" xmlns:rtns="http://shipment.dto" soapAction="" >
        <parameter qname="pns:shipmentId" xmlns:pns="http://shipment_station.service" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="registerShipment" qname="operNS:registerShipment" xmlns:operNS="http://shipment_station.service" returnQName="retNS:registerShipmentReturn" xmlns:retNS="http://shipment_station.service" returnType="rtns:ShipmentDTO" xmlns:rtns="http://shipment.dto" soapAction="" >
        <parameter qname="pns:parts" xmlns:pns="http://shipment_station.service" type="tns:PartDTO" xmlns:tns="http://part.dto"/>
        <parameter qname="pns:receiverFirstName" xmlns:pns="http://shipment_station.service" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:receiverLastName" xmlns:pns="http://shipment_station.service" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getShipment registerShipment getAllShipments"/>

      <typeMapping
        xmlns:ns="http://shipment.dto"
        qname="ns:ShipmentDTO"
        type="java:dto.shipment.ShipmentDTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://shipment_station.service"
        qname="ns:ArrayOf_tns2_PartDTO"
        type="java:dto.part.PartDTO[]"
        innerType="cmp-ns:PartDTO" xmlns:cmp-ns="http://part.dto"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://part.dto"
        qname="ns:PartDTO"
        type="java:dto.part.PartDTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
