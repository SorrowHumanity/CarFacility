<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DismantleStationServiceService WSDL service -->

  <service name="DismantleStationService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://dismantle_station.service.server"/>
      <parameter name="wsdlServiceElement" value="DismantleStationServiceService"/>
      <parameter name="schemaQualified" value="http://dismantle_station.service.server,http://part.dto"/>
      <parameter name="wsdlServicePort" value="DismantleStationService"/>
      <parameter name="className" value="server.service.dismantle_station.DismantleStationServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="DismantleStationService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getParts" qname="operNS:getParts" xmlns:operNS="http://dismantle_station.service.server" returnQName="retNS:getPartsReturn" xmlns:retNS="http://dismantle_station.service.server" returnType="rtns:PartDTO" xmlns:rtns="http://part.dto" soapAction="" >
        <parameter qname="pns:carChassisNumber" xmlns:pns="http://dismantle_station.service.server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getParts"/>

      <typeMapping
        xmlns:ns="http://part.dto"
        qname="ns:PartDTO"
        type="java:dto.part.PartDTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
